{
	"name": "cypress-automation",
	"version": "1.0.0",
	"main": "index.js",
	"engines": {
		"node": ">=18.x.x"
	},
	"devDependencies": {
		"cypress": "13.11.0",
		"cypress-dotenv": "^2.0.0",
		"cypress-iframe": "^1.0.1",
		"cypress-multi-reporters": "^1.6.3",
		"cypress-split": "^1.3.4",
		"cypress-wait-until": "^1.7.2",
		"dotenv": "^16.0.1",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-prettier": "^4.2.1",
		"lint-staged": "^13.1.0",
		"prettier": "2.8.1",
		"react-scripts": "^5.0.1"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"cypress:open": "yarn create-accounts && cypress open",
		"cypress:run": "cypress run",
		"cypress:parallel": "cypress-parallel -s cypress:run -t 2 -d './cypress/e2e/trigger/**'",
		"prepare-accounts": "cypress run --spec cypress/e2e/trigger/service/prepare-accounts.cy.js --browser chrome",
		"create-accounts": "node script/check-file-existence.js && node script/create-account.js temporaryLender && node script/create-account.js temporaryBorrower && node script/create-account.js temporaryPartner && node script/create-account.js temporaryTeamMember",
		"check-file-existence": "node script/check-file-existence.js",
		"create-acc-by:lender": "node script/create-account.js temporaryLender",
		"create-acc-by:borrower": "node script/create-account.js temporaryBorrower",
		"create-acc-by:partner": "node script/create-account.js temporaryPartner",
		"create-acc-by:team-member": "node script/create-account.js temporaryTeamMember"
	},
	"author": "",
	"license": "ISC",
	"description": "",
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"cypress-parallel": "^0.12.0",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.19.2/xlsx-0.19.2.tgz"
	}
}
